{
  "name": "JetHome JetHub mqtt-io peripheral exposer",
  "version": "dev",
  "slug": "jethub-mqtt-io",
  "description": "Expose JetHome JetHub peripheral (relays, inputs, etc..) via mqtt-io",
  "uart": true,
  "url": "https://github.com/jethome-ru/hassio-addon-jethub-mqtt-io",
  "services": [
    "mqtt:need"
  ],
  "arch": [
    "aarch64"
  ],
  "homeassistant": "2021.8.8",
  "ports": {},
  "ports_description": {},
  "privileged": [
    "SYS_RAWIO"
  ],
  "full_access": true,
  "apparmor": false,
  "options": {
    "jethub_model": "auto",
    "mqtt": {
      "base_topic": "jethub-mqtt-io"
    }
  },
  "schema": {
    "jethub_model": "str",
    "mqtt": {
      "base_topic": "str",
      "server": "str?",
      "user": "str?",
      "password": "str?"
    }
  }
}
